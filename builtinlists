l=[10,20,30,40,50]
print(len(l))
print(max(l))
print(min(l))
print(all(l))
print(any(l))

l1=[10,20,0,30]
print(all(l1))
print(any(l1))

l2=[0,0,0,0]
print(all(l1))
print(any(l1))

l3=[0,20,0,0]
print(all(l1))
print(any(l1))

l4=[60,20,10,50]
print(sorted(l4))

l=[10,20,30]
print(l)
l.append(40)
print(l)
l.extend([60,70])
print(l)
l.insert(2,35)
print(l)
l.pop()
print(l)
l.remove(35)
print(l)
l.clear()
print(l)
del l
print(l)
